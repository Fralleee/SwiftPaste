{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard-with-typescript",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": [1, "never"],
    "quotes": [1, "double"],
    "max-len": [1, { "code": 180 }],
    "comma-dangle": [1, "never"],
    "space-before-function-paren": [1, "never"]
  }
}

