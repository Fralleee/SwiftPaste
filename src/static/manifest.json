{
  "manifest_version": 3,
  "name": "SwiftPaste",
  "version": "0.4.1",
  "description": "SwiftPaste: A Chrome extension optimizing text inputs by providing quick access to a categorized text storage, based on context.",
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/16.png",
      "24": "images/24.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "64": "images/64.png",
      "128": "images/128.png",
      "256": "images/256.png"
    }
  },
  "icons": {
    "16": "images/16.png",
    "24": "images/24.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "64": "images/64.png",
    "128": "images/128.png",
    "256": "images/256.png"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "scripting",
    "activeTab"
  ],
  "commands": {
    "openSwiftPaste": {
      "suggested_key": {
        "default": "Ctrl+Shift+Space"
      },
      "description": "Open \"SwiftPaste\" popup."
    }
  }
}